{
  "manifest_version": 2,
  "name": "Kelime Geliştirici",
  "version": "1.0",
  "description": "İngilizce kelimelerle pratik yapma eklentisi",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/logo.png"  
  },
  "icons": {
    "16": "img/logo.png",  
    "128": "img/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
